{
  "name": "gamemakerbot",
  "version": "4.0.0",
  "description": "The bot used by the /r/GameMaker Discord server.",
  "main": "./gmbot",
  "license": "MIT",
  "scripts": {
    "docs": "docsify serve ./docs",
    "g": "ts-node ./tools/generate.ts",
    "install": "ts-node ./tools/post-install.ts",
    "lint": "tslint \"gmbot.ts\" && tslint \"src/**/*.ts\"",
    "start": "ts-node gmbot.ts",
    "test": "mocha -r ts-node/register \"src/**/*.spec.ts\" --exit"
  },
  "precommit": {
    "silent": false,
    "run": [
      "test",
      "lint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/christopherwk210/gm-bot"
  },
  "keywords": [
    "discord",
    "bot",
    "client",
    "node",
    "discordapp"
  ],
  "author": "Chris Anselmo <christopherwk210@gmail.com>",
  "homepage": "https://bitbucket.org/christopherwk210/gm-bot",
  "dependencies": {
    "@types/async": "^2.0.50",
    "@types/cheerio": "^0.22.9",
    "@types/concat-stream": "^1.6.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/js-beautify": "1.8.0",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^10.11.4",
    "@types/puppeteer": "^1.9.0",
    "async": "^2.6.1",
    "body-parser": "^1.18.3",
    "bufferutil": "^4.0.0",
    "cheerio": "^1.0.0-rc.2",
    "concat-stream": "^1.6.2",
    "cors": "^2.8.5",
    "discord.js": "^11.4.2",
    "docsify-cli": "^4.3.0",
    "exec-buffer": "^3.2.0",
    "express": "^4.16.4",
    "gifsicle": "^3.0.4",
    "glob": "^7.1.3",
    "jimp": "^0.5.3",
    "js-beautify": "^1.8.6",
    "puppeteer": "^1.9.0",
    "q": "^1.5.1",
    "request": "^2.88.0",
    "string-similarity": "^2.0.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.1",
    "youtube-dl": "^1.12.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "pre-commit": "^1.2.2",
    "prompts": "^1.1.1",
    "tslint": "^5.11.0",
    "tslint-no-unused-expression-chai": "^0.1.4"
  },
  "engines": {
    "node": ">=9.2.0"
  }
}
